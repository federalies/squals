{
  "name": "squals",
  "version": "0.1.0",
  "description": "A Web-friendly cloud generator making simple to share and integrate cloud components",
  "main": "lib/src/index.js",
  "typings": "lib/src/index.d.ts",
  "author": "Eric D Moore",
  "license": "MIT",
  "directories": {
    "example": "examples",
    "test": "tests",
    "lib": "src"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/federalies/squals"
  },
  "scripts": {
    "clearDist": "rm -rf dist/",
    "debug": "node --inspect --inspect-brk",
    "debug:jest": "npm run debug -- /node_modules/jest/bin/jest.js --env=node --colors --coverage",
    "jest": "jest --env=node --colors --coverage",
    "test": "npm run clearDist && npm run jest -- test",
    "test:watch": "npm run test -- --watchAll",
    "test:cli": "npm run jest",
    "docs": "typedoc --out docs && open ./docs/index.html",
    "mjs": "tsc -d && mv dist/*.js dist/copee.mjs",
    "cjs": "tsc -m commonjs",
    "umd": "tsc -m umd",
    "build": "npm run cjs && npm run mjs && npm run umd"
  },
  "keywords": [
    "federalies",
    "generator",
    "aws",
    "gcloud",
    "azure",
    "cloudformation",
    "templates",
    "cloud-templates",
    "infrastructure"
  ],
  "dependencies": {
    "@sandfox/arn": "^2.1.0",
    "aws-sdk": "^2.441.0",
    "deep-equal": "^1.0.1",
    "esm": "^3.2.4",
    "getopts": "^2.2.3",
    "graphql": "^14.4.1",
    "handlebars": "^4.1.2",
    "js-yaml": "^3.13.1",
    "lodash-es": "^4.17.21",
    "nanoid": "^2.0.3",
    "openapi-types": "^1.3.5",
    "random-word": "^2.0.0",
    "randoma": "^1.2.0",
    "s3-url": "^0.2.2",
    "superstruct": "^0.6.1"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-typescript": "^7.3.3",
    "@types/aws-sdk": "^2.7.0",
    "@types/deep-equal": "^1.0.1",
    "@types/graphql": "^14.2.2",
    "@types/hapi__joi": "^15.0.2",
    "@types/jest": "^24.0.9",
    "@types/joi": "^14.3.3",
    "@types/js-yaml": "^3.12.1",
    "@types/lodash": "^4.14.121",
    "@types/lodash-es": "^4.17.2",
    "@types/nanoid": "^2.0.0",
    "@types/node": "^11.13.8",
    "@types/randoma": "^1.2.0",
    "@types/superstruct": "^0.5.0",
    "acorn": "^6.2.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.8.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "dotenv": "^6.2.0",
    "eslint": "^5.13.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jest": "^22.2.2",
    "eslint-plugin-jsdoc": "^4.1.1",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-utils": ">=1.4.1",
    "jest": "^24.1.0",
    "lodash": "^4.17.15",
    "prettier": "^1.18.2",
    "ts-jest": "^24.0.0",
    "ts-node": "^8.1.0",
    "typedoc": "^0.15.0",
    "typedoc-plugin-external-module-name": "^2.1.0",
    "typedoc-plugin-internal-external": "^2.0.2"
  },
  "futureIdeas": {
    "deps": {
      "rus-diff": "^1.1.0"
    }
  },
  "old": {
    "deps": {
      "@hapi/joi": "^15.1.0",
      "acorn": "^6.1.1"
    },
    "dev": {
      "@loopback/openapi-spec-builder": "^1.2.5",
      "typedoc-plugin-single-line-tags": "^1.0.0",
      "ajv": "^6.9.1",
      "ajv-cli": "^3.0.0"
    }
  }
}
